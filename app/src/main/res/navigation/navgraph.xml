<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navgraph"
    app:startDestination="@id/startPageFragment">

    <fragment
        android:id="@+id/startPageFragment"
        android:name="com.example.meet_n_music.StartPageFragment"
        android:label="fragment_start_page"
        tools:layout="@layout/fragment_start_page" >
        <action
            android:id="@+id/action_startPageFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_startPageFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment" />
        <action
            android:id="@+id/action_startPageFragment_to_creatingEventFragment"
            app:destination="@id/creatingEventFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.meet_n_music.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_startPageFragment"
            app:destination="@id/startPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/userProfileFragment"
        android:name="com.example.meet_n_music.UserProfileFragment"
        android:label="UserProfileFragment" >
        <action
            android:id="@+id/action_userProfileFragment_to_changePasswordFragment"
            app:destination="@id/changePasswordFragment" />
        <action
            android:id="@+id/action_userProfileFragment_to_changeEmailFragment"
            app:destination="@id/changeEmailFragment" />
    </fragment>
    <fragment
        android:id="@+id/changePasswordFragment"
        android:name="com.example.meet_n_music.ChangePasswordFragment"
        android:label="ChangePasswordFragment" />
    <fragment
        android:id="@+id/changeEmailFragment"
        android:name="com.example.meet_n_music.ChangeEmailFragment"
        android:label="ChangeEmailFragment" />
    <fragment
        android:id="@+id/creatingEventFragment"
        android:name="com.example.meet_n_music.CreatingEventFragment"
        android:label="CreatingEventFragment" >
        <action
            android:id="@+id/action_creatingEventFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment" />
        <action
            android:id="@+id/action_creatingEventFragment_to_viewEventFragment"
            app:destination="@id/viewEventFragment" />
    </fragment>
    <fragment
        android:id="@+id/viewEventFragment"
        android:name="com.example.meet_n_music.ViewEventFragment"
        android:label="fragment_view_event"
        tools:layout="@layout/fragment_view_event" />

</navigation>